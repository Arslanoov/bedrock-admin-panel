<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Login</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        {% for user in users %}
            <tr>
                <td>{{ user.id }}</td>
                <td>{{ user.login }}</td>
                <td>{{ user.status }}</td>
            </tr>
        {% endfor %}
    </tbody>
</table>