{
  "name": "furious/web-skeleton",
  "type": "project",
  "license": "MIT",
  "require": {
    "php" : ">=7.4",
    "psr/http-message": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "psr/container": "^1.0",
    "furious/psr7": "^1.3",
    "furious/router": "1.0.*",
    "furious/psr15": "^1.0",
    "furious/container": "^1.0",
    "furious/http-runner": "^0.1",
    "cycle/orm": "^1.2",
    "twig/twig": "^3.0",
    "cycle/annotated": "^2.0",
    "cycle/migrations": "^1.0",
    "symfony/console": "^5.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "App\\" : "src/App",
      "Framework\\": "src/Framework",
      "Infrastructure\\": "src/Infrastructure",
      "Domain\\": "src/Domain"
    }
  },
  "scripts": {
    "serve": "php -S 0.0.0.0:8093 -t public"
  }
}
